<!DOCTYPE html>
<html lang="en">
  <head>
    <title>DJ's Web Portfolio</title>
    <meta name="description" content="" />
    {{>head}}
  </head>

  <body>
    <div class="blocks-page">
      <section id="hero_section">
        <button class="nav-btn open-btn">
            <i class="fas fa-bars"></i>
        </button>
          <div class="nav">
            <div class="nav nav-white">
                <button class="nav-btn close-btn">
                  <i class="fas fa-times"></i>
                </button>
              <ul class="list nav-links">
                            {{#page.header_nav.nav5.show_or_hide}}
                              <li class="nav-item">
                                  <a class="nav-link"  href="/profile">{{page.header_nav.nav5.text}}</a>
                              </li>
                              {{/page.header_nav.nav5.show_or_hide}}
                            {{#page.header_nav.nav1.show_or_hide}}
                            <li class="nav-item">
                                <a class="nav-link" href="/#my-projects">{{page.header_nav.nav1.text}}</a>
                            </li>
                            {{/page.header_nav.nav1.show_or_hide}}

                                           {{#page.header_nav.nav2.show_or_hide}}
                            <li class="nav-item">
                                <a class="nav-link"  href="/#tech-skills">{{page.header_nav.nav2.text}}</a>
                            </li>
                            {{/page.header_nav.nav2.show_or_hide}}

                            {{#page.header_nav.nav3.show_or_hide}}
                            <li class="nav-item">
                                <a class="nav-link" target="_blank"  href="https://storage.turbo360.co/my-portfolio-fctgxi/resume-dev.pdf">{{page.header_nav.nav3.text}}</a>
                            </li>
                            {{/page.header_nav.nav3.show_or_hide}}

                            {{#page.header_nav.nav4.show_or_hide}}
                            <li class="nav-item">
                                <a class="nav-link"  href="/#contact-form">{{page.header_nav.nav4.text}}</a>
                            </li>
                            {{/page.header_nav.nav4.show_or_hide}}
              </ul>
            </div>
          </div>

        <div class="animated-gradient-red  py-3">
        <section id="mobile-navbar">
          <nav class="mobile-navbar">
            <div class="mobile-navbar-container">
                      <ul>
                       {{#page.header_nav.nav5.show_or_hide}}
                              <li class="nav-item-m">
                                  <a class="nav-link"  href="/">{{page.header_nav.nav5.text}}</a>
                              </li>
                              {{/page.header_nav.nav5.show_or_hide}}
                              {{#page.header_nav.nav1.show_or_hide}}
                              <li class="nav-item-m">
                                  <a class="nav-link" href="/#my-projects">{{page.header_nav.nav1.text}}</a>
                              </li>
                              {{/page.header_nav.nav1.show_or_hide}}
  
                                             {{#page.header_nav.nav2.show_or_hide}}
                              <li class="nav-item-m">
                                  <a class="nav-link"  href="/#tech-skills">{{page.header_nav.nav2.text}}</a>
                              </li>
                              {{/page.header_nav.nav2.show_or_hide}}
  
                              {{#page.header_nav.nav3.show_or_hide}}
                              <li class="nav-item-m">
                                  <a class="nav-link" target="_blank"  href="https://storage.turbo360.co/my-portfolio-fctgxi/resume-dev.pdf">{{page.header_nav.nav3.text}}</a>
                              </li>
                              {{/page.header_nav.nav3.show_or_hide}}
  
                              {{#page.header_nav.nav4.show_or_hide}}
                              <li class="nav-item-m">
                                  <a class="nav-link"  href="/#contact-form">{{page.header_nav.nav4.text}}</a>
                              </li>
                              {{/page.header_nav.nav4.show_or_hide}}
                      </ul>
            </div>
          </nav>
        </section>
          <div id="boxes" class="boxes big"></div>
          <h2>{{ page.hero.greeting }}</h2>
        </div>
      </section>

      <section id="my-projects" class="animated-gradient-red-inverse py-5 py-md-7">
          <div class="container py-md-5">
            <div class="row block-header">
              <div class="col-sm-8 mx-auto text-center">
                <h1  class="display-4 mt-2 uline">{{ page.projects.header }}</h1>
                <hr />
                <h3>{{ page.projects.subheader }}</h3>
              </div>
            </div>
            <div
              class="row mt-5 aos-init aos-animate"
              data-aos="fade-up"
              data-aos-duration="1000"
            >
              {{#projects}}
              <div class="col-md-4 col-sm-12 mb-2r">
                <a href="/project/{{ slug }}">
                  <div class="position-relative project">
                    <div class="image position-relative">
                      <img
                        src="{{{image}}}=s650-c"
                        class="img-fluid w-100 d-block"
                      />
                      <div class="overlay-box">
                        <div class="overlay-inner overlay-content">
                          <h5 class="mb-0">{{ name }}</h5>
                          <p>{{ category }}</p>
                        </div>
                      </div>
                    </div>
                  </div>
                </a>
              </div>
              {{/projects}}
            </div>
          </div>
      </section>

      {{! Technical Skills  }}

      <section id="tech-skills" class="py-md-5 block">
        <div class="container">
          <div class="row block-header">
            <div class="col-sm-8 mx-auto text-center">
              <h1 class="display-4 mt-2">Technical Skills</h1>
              <hr />
            </div>
          </div>

          {{! Stacks & Techs  }}

          <div  class="row mt-5">
            {{#page.services}}
            <div class="col-xl-3 service mb-5 aos-init aos-animate">
              <img class="icon mr-4" src="{{{img_feature}}}" />
              <div class="content">
                <h5>{{ name }}</h5>
                <p>{{ desc }}</p>
              </div>
            </div>
            {{/page.services}}
          </div>
        </div>
      </section>

      <!-- Contact Form -->
      <section id="contact-form" class="animated-gradient-red-inverse-delayed contact py-md-5 block text-center contact-block">
        <div class="container">
          <div class="row block-header">
            <div class="col-sm-8 mx-auto text-center">
              <h1 class="display-4 mt-2 section-title" 
              id="contact-section-header">
                {{ page.contact.header }}
              </h1>
              <hr />
              <p class="lead contact-section-subheader  ">{{ page.contact.subheader }}</p>
              <p class="lead contact-section-subheader  " style="font-size: xx-large;">OR</p>
              <p class="lead contact-section-subheader  ">{{ page.contact.prof_email }}</p>
            </div>
          </div>
          <div class="row mt-5">
            <form class="col-md-4 col-xl-4 mx-auto input-border">
              <div class="form-group">
                <input
                  type="text"
                  class="form-control form-control-lg"
                  id="contact-form-name"
                  placeholder="Name"
                />
              </div>
              <div class="form-group">
                <input
                  type="text"
                  id="contact-form-email"
                  class="form-control form-control-lg"
                  placeholder="Email"
                />
              </div>
              <div class="form-group">
                <textarea
                  type="text"
                  class="form-control form-control-lg"
                  id="contact-form-message"
                  rows="3"
                  placeholder="Message"
                ></textarea>
              </div>
              <button
                id="btn-contact-submit"
                class="btn btn-block btn-lg btn-info"
              >
                Send
              </button>
            </form>
          </div>
        </div>
      </section>
      

      <footer class="pt-3">
          <div class="container pt-5 pb-5">
              <div class="row">      
                  <div id="contact-ctx"  class="col-md-12 text-center gradient-div">
                    <h1 id="social-media">@</h1>

                      <ul class="list-inline social social-white-alt social-rounded social-sm mb-0">
                          {{#page.social.twitter.show_or_hide}}
                          <li class="list-inline-item"><a href="{{{page.social.twitter.link}}}" target="_blank"><i class="fa fa-twitter"></i></a></li>
                          {{/page.social.twitter.show_or_hide}}

                          {{#page.social.linkedin.show_or_hide}}
                          <li class="list-inline-item"><a href="{{{page.social.linkedin.link}}}" target="_blank"><i class="fa fa-linkedin"></i></a></li>
                          {{/page.social.linkedin.show_or_hide}}

                          {{#page.social.github.show_or_hide}}
                          <li class="list-inline-item"><a href="{{{page.social.github.link}}}" target="_blank"><i class="fa fa-github"></i></a></li>
                          {{/page.social.github.show_or_hide}}

                      </ul>
                  </div>
              </div>
          </div>
      </footer>
    </div>

    {{>scripts}}
    <script src="{{{cdn}}}/js/contact.js"></script>

    {{! My Custom JS  }}
    <script src="{{{cdn}}}/dist/js/dj_custom.js"></script>
  </body>
</html>